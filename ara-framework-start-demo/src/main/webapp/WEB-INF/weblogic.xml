<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app
        xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-web-app"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd
		http://xmlns.oracle.com/weblogic/weblogic-web-app
		http://xmlns.oracle.com/weblogic/weblogic-web-app/1.4/weblogic-web-app.xsd">
    <wls:container-descriptor>
        <!-- 指示优先从部署项目的WEB-INF/lib目录下加载jar包 -->
        <!--<wls:prefer-web-inf-classes>true</wls:prefer-web-inf-classes>-->
        <wls:prefer-application-packages>
            <wls:package-name>ch.qos.logback.*</wls:package-name>
            <wls:package-name>com.alibaba.*</wls:package-name>
            <wls:package-name>com.aratek.*</wls:package-name>
            <wls:package-name>com.fasterxml.jackson.*</wls:package-name>
            <wls:package-name>com.fasterxml.classmate.*</wls:package-name>
            <wls:package-name>com.github.pagehelper.*</wls:package-name>
            <wls:package-name>com.google.common.*</wls:package-name>
            <wls:package-name>com.google.thirdparty.*</wls:package-name>
            <wls:package-name>oracle.*</wls:package-name>
            <wls:package-name>com.sun.mail.*</wls:package-name>
            <wls:package-name>javax.mail.*</wls:package-name>
            <wls:package-name>org.apache.commons.*</wls:package-name>
            <wls:package-name>io.jsonwebtoken.*</wls:package-name>
            <wls:package-name>io.swagger.*</wls:package-name>
            <wls:package-name>springfox.documentation.*</wls:package-name>
            <wls:package-name>com.sun.activation.*</wls:package-name>
            <wls:package-name>javax.activation.*</wls:package-name>
            <wls:package-name>javax.persistence.*</wls:package-name>
            <wls:package-name>javax.validation.*</wls:package-name>
            <wls:package-name>net.sf.ehcache.*</wls:package-name>
            <wls:package-name>org.apache.shiro.*</wls:package-name>
            <wls:package-name>freemarker.*</wls:package-name>
            <wls:package-name>org.hibernate.validator.*</wls:package-name>
            <wls:package-name>org.apache.ibatis.*</wls:package-name>
            <wls:package-name>org.mybatis.spring.*</wls:package-name>
            <wls:package-name>org.slf4j.*</wls:package-name>
            <wls:package-name>org.springframework.*</wls:package-name>
            <wls:package-name>tk.mybatis.spring.*</wls:package-name>
            <wls:package-name>tk.mybatis.mapper.*</wls:package-name>
        </wls:prefer-application-packages>

        <wls:show-archived-real-path-enabled>true</wls:show-archived-real-path-enabled>
    </wls:container-descriptor>

    <wls:context-root>/springbootdemo/api</wls:context-root>
</wls:weblogic-web-app>